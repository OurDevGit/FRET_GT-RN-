{
  "name": "GuitarTunes",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start":
      "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "test": "jest",
    "stylekit":
      "node makeStyleKit.js -j /com/optek/guitartunes/GuitarTunesStyleKit.java -s src/components/StyleKit/styleKitComponents.js",
    "clean":
      "watchman watch-del-all && rm -rf $TMPDIR/react-* && cd android && ./gradlew clean && cd ..",
    "cleanjs":
      "watchman watch-del-all && rm -rf node_modules && npm install && rm -rf $TMPDIR/react-*",
    "build":
      "cd android && ./gradlew assembleRelease && cd .. && open android/app/build/outputs/apk/",
    "dev": "react-native run-android",
    "rel": "react-native run-android --variant=release",
    "lint": "eslint -c .eslintrc - ext .js src"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "base64-arraybuffer": "^0.1.5",
    "fuse.js": "^3.1.0",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "midi-file-parser": "^1.0.0",
    "midi-player-js": "^1.1.1",
    "midiconvert": "^0.4.1",
    "prop-types": "^15.5.10",
    "re-reselect": "^0.5.0",
    "react": "^16.0.0-beta.5",
    "react-native": "^0.49.3",
    "react-native-billing": "^2.3.1",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-responsive": "^1.0.1",
    "react-native-sentry": "^0.30.2",
    "react-native-sound": "big-swing/react-native-sound",
    "react-native-tab-view": "0.0.69",
    "react-native-video": "^2.0.0",
    "react-redux": "^5.0.5",
    "recompose": "^0.25.1",
    "redux": "^3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.3",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "ajv": "^5.2.3",
    "babel-eslint": "^7.2.3",
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "2.0.0",
    "commander": "^2.11.0",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-react": "^7.4.0",
    "jest": "20.0.4",
    "react-test-renderer": "16.0.0-beta.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
